<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="My custom checks" priority="1600">
        <description>%MyCustomChecks.desc</description>

        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
            <quickfix classname="net.sf.eclipsecs.sample.checks.MethodLimitQuickfix" />
        </rule-metadata>
        
        <rule-metadata name="%HalsteadMetrics.name" internal-name="HalsteadMetrics" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.HalsteadMetrics" />
        	<description>%HalsteadMetrics.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%MaintainabilityIndex.name" internal-name="MaintainabilityIndex" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.MaintainabilityIndex" />
        	<description>%MaintainabilityIndex.desc</description>
        </rule-metadata>
        	
        <rule-metadata name="%CommentCounter.name" internal-name="CommentCounter" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.CommentCounter" />
        	<description>%CommentCounter.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%VariableCounter.name" internal-name="VariableCounter" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.VariableCounter" />
        	<description>%VariableCounter.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%LoopingStatementCounter.name" internal-name="LoopingStatementCounter" parent="TreeWalker">
        	<alternative-name internal-name="net.sf.eclipsecs.sample.checks.LoopingStatementCounter" />
        	<description>%LoopingStatementCounter.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%CastCounter.name" internal-name="CastCounter" parent="TreeWalker">
        	<alternative-name interal-name="net.sf.eclipsecs.sample.checks.CastCounter" />
        	<description>%CastCounter.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%ExpressionCounter.name" internal-name="ExpressionCounter" parent="TreeWalker">
        	<alternative-name interal-name="net.sf.eclipsecs.sample.checks.ExpressionCounter" />
        	<description>%ExpressionCounter.desc</description>
        </rule-metadata>
        
        <rule-metadata name="%MethodCounter.name" internal-name="MethodCounter" parent="TreeWalker">
        	<alternative-name interal-name="net.sf.eclipsecs.sample.checks.MethodCounter" />
        	<description>%MethodCounter.desc</description>
        </rule-metadata>
        
    </rule-group-metadata>
</checkstyle-metadata>
